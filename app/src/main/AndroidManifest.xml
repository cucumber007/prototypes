<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.cucumber007.prototypes"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menu.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activities.recycler.RecyclerActivity">
        </activity>
        <activity android:name=".activities.butterknife.ButterknifeActivity">
        </activity>
        <activity android:name=".activities.mvc.MvcActivity">
        </activity>
        <activity android:name=".activities.rxjava.RxJavaActivity">
        </activity>
        <activity android:name=".activities.lambda.LambdaActivity">
        </activity>
        <activity android:name=".activities.retrofit.RetrofitActivity">
        </activity>
        <activity android:name=".activities.views.ViewsActivity">
        </activity>
        <activity android:name=".activities.xml_drawables.XmlDrawableActivity">
        </activity>
    </application>

</manifest>
